// Q1 Take values of length and breath of a rectangle from user and check if it is a square or not?
#include<iostream>
using namespace std;
int main()
{
    int l,b;
    cout<<"Enter the length"<<endl;
    cin>>l;
    cout<<"Enter the breadth"<<endl;
    cin>>b;
    
    if(l==b)
    {
        cout<<"IT IS A SQUARE"<<endl;
    }
    else
    {
              cout<<"IT IS NOT A SQUARE"<<endl;
  
    }
}
